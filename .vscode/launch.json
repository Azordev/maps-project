{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "name": "vscode-jest-tests",
          "request": "launch",
          "args": [
              "test",
              "--runInBand",
              "--no-cache"
          ],
          "cwd": "${workspaceFolder}",
          "console": "internalConsole",
          "internalConsoleOptions": "openOnSessionStart",
          "disableOptimisticBPs": true,
          "runtimeExecutable":
              "${workspaceFolder}/node_modules/.bin/react-scripts",
          "protocol": "inspector"
      }
  ]
}
